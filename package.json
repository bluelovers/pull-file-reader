    "url": "git+https://github.com/bluelovers/pull-file-reader.git"
  },
  "license": "ISC",
  "author": "Alan Shaw",
  "main": "index.js",
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "prepublish:lockfile": "npx sync-lockfile .",
